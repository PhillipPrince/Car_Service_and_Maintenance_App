<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carservice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarService">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity
            android:name=".Car_Details"
            android:exported="false" />
        <activity
            android:name=".MyCarDetails"
            android:exported="false" />
        <activity
            android:name=".CarExpenses"
            android:exported="false" />
        <activity
            android:name=".Notifications"
            android:exported="false" />
        <activity
            android:name=".ViewMechanicDetails"
            android:exported="false" />
        <activity
            android:name=".addMechanic"
            android:exported="false" />
        <activity
            android:name=".mechanic"
            android:exported="false"
            android:label="@string/title_activity_mechanic"
            android:theme="@style/Theme.CarService.NoActionBar" />
        <activity
            android:name=".car"
            android:exported="false"
            android:label="@string/title_activity_car"
            android:theme="@style/Theme.CarService.NoActionBar" />
        <activity
            android:name=".myservice"
            android:exported="false"
            android:label="@string/title_activity_myservice"
            android:theme="@style/Theme.CarService.NoActionBar" />
        <activity android:name=".home" />
        <activity android:name=".newUser" />
        <activity android:name=".service" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>